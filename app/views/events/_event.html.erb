<div class='col-md-6 col-lg-4 mb-3'>
  <%= link_to(event_path(event)) do %>
  <div class='card w'>
    <img src='https://s3.amazonaws.com/nfeventimages/uploads/event/event_card/1/Lafayette_LA.jpg' class='card-img' />
    <div class='card-img-overlay overlay-shadow'>
      <h4 class="card-title"><%= event_name(event) %></h4>
      <h5><%= event.event_start.strftime("%b - %d") %></h5>
    </div>
  </div>
  <% end %>
</div>